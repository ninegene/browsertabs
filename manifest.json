{
  "name": "Save Tabs",
  "description": "",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'unsafe-eval'; object-src 'self'",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Save Tabs"
  },
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
